{
    "metrics": [
        {
            "name": "best_practices",
            "airtable_name": "Best Practices badge",
            "description": "Does repo have a best practices badge?",
            "url": "https://chaoss.community/kb/metric-open-source-security-foundation-openssf-best-practices-badge/",
            "what": "binary",
            "scale": "none",
            "better": "higher"
        },
        {
            "name": "burstiness",
            "airtable_name": "Development burstiness",
            "description": "A measure of the 'burtiness' in all development activity (code, issues, comments). Lower numbers reflect more consistent development activity.",
